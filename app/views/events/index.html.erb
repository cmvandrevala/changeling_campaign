<% provide(:title, 'Event Index')%>

<h1>Event Index</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
